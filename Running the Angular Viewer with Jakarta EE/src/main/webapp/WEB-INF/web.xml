<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Running the Angular Viewer with Jakarta EE</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StimulsoftAngularViewer</servlet-name>
    <servlet-class>com.stimulsoft.demo.StiAngularViewerActionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StimulsoftAngularViewer</servlet-name>
    <url-pattern>/angular_viewer</url-pattern>
  </servlet-mapping>
</web-app>